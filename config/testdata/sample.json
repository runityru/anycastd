{
  "announcer": {
    "router_id": "10.3.3.3",
    "local_address": "10.0.0.1",
    "local_as": 65999,
    "routes": [
      "10.0.0.128/32"
    ],
    "peers": [
      {
        "name": "some_router_1",
        "remote_address": "10.0.0.252",
        "remote_as": 65000
      },
      {
        "name": "some_router_2",
        "remote_address": "10.0.0.253",
        "remote_as": 65000
      }
    ]
  },
  "services": [
    {
      "name": "http",
      "check_operator": "and",
      "check_interval": "10s",
      "checks": [
        {
          "kind": "dns_lookup",
          "spec": {
            "query": "example.com",
            "resolver": "127.0.0.1",
            "tries": 3,
            "interval": "100ms"
          }
        },
        {
          "kind": "http_2xx",
          "spec": {
            "address": "127.0.0.1:8080",
            "path": "/",
            "tries": 3,
            "interval": "100ms",
            "timeout": "2s"
          }
        },
        {
          "kind": "assigned_address",
          "spec": {
            "interface": "dummy0",
            "ipv4": "10.0.0.128"
          }
        }
      ]
    }
  ],
  "metrics": {
    "enabled": true,
    "address": "127.0.0.1:9090"
  }
}
